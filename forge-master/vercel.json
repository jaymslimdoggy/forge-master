
{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/index.tsx",
      "destination": "/index.tsx"
    },
    {
      "source": "/App.tsx",
      "destination": "/App.tsx"
    },
    {
      "source": "/types.ts",
      "destination": "/types.ts"
    },
    {
      "source": "/constants.ts",
      "destination": "/constants.ts"
    },
    {
      "source": "/services/(.*)",
      "destination": "/services/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(ts|tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}
